{
  "name": "react-starter-boilerplate-hmr",
  "version": "1.0.0",
  "description": "React sandbox with Hot Module Replacement, React 16, Webpack 4, Emotion CSS",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --env.env=dev",
    "dev:bundleanalyzer": "yarn dev -- --env.addons=bundleanalyzer",
    "prebuild": "rimraf docs",
    "build": "cross-env NODE_ENV=production webpack -p --env.env=prod",
    "build:bundleanalyzer": "yarn build -- --env.addons=bundleanalyzer"
  },
  "dependencies": {
    "emotion": "^9.2.6",
    "react-emotion": "^9.2.6",
    "emotion-server": "^9.2.6",
    "emotion-theming": "^9.2.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-delay-render": "^0.1.2",
    "react-dom": "^16.4.1",
    "react-imported-component": "^1.0.4",
    "react-router-dom": "^4.3.1",
    "semantic-ui-react": "^0.75.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-emotion": "^9.2.6",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
